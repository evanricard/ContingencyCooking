
@{
    ViewBag.Title = "AllResults";
}


@using Microsoft.AspNet.Identity;
@using Microsoft.AspNet.Identity.Owin;
<h2>All Results</h2>

<table class="table">
    <thead><tr><th>Username</th><th>Title</th><th>Difficulty</th><th>Rating</th></tr></thead>
    @for (int i = 0; i < ViewBag.Results.Count; i++)
    {
        <tr><td>@ViewBag.Emails[i]</td><td>@ViewBag.Results[i].Recipe.Title</td><td>@ViewBag.Results[i].Difficulty</td><td>@ViewBag.Results[i].Rating</td></tr>
    }
</table>